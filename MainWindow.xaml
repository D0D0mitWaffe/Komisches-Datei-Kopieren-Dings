        <Label Content="From:" Margin="40,77,623,200"/>
        <TextBox x:Name="FromPathBox" IsReadOnly="True" Margin="44,109,451,280" MouseDown="FromPathBox_MouseDown"/>
        <Label Content="To:" Margin="417,77,267,200"/>
